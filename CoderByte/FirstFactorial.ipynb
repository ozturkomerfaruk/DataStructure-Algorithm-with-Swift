        "def FirstFactorial(num):\n",
        "\n",
        "  intNum = int(num)\n",
        "\n",
        "  if (intNum == 1):\n",
        "    return intNum\n",
        "  else:\n",
        "    return intNum * FirstFactorial(intNum-1)\n",
        "\n",
        "# keep this function call here \n",
        "print(FirstFactorial(input()))"
      ]
